# Initialize dictionary with random husky players
huskies = { 3: 'Molden', 9: 'Morris', 2: 'Gordon', 5: 'McGrew', 6: 'Newton', 8: 'Spiker'};

# Print type of huskies
print("Type:", type(huskies), "\n");

# Print values and keys 
print("All values:", huskies, "\n");

# Print only keys
print("Keys:", list(huskies), "\n");

# Print only keys sorted
print("Keys sorted:", sorted(huskies), "\n");

# Iterate through dictionary
for key, value in huskies.items():
    print("Number:", key, "Name:", value);
print();

# Search for value
print("Using 'huskies.get(5)' returns:", huskies.get(5), "\n");

# Dictionary length
print("Dictionary length:", len(huskies), "\n");

print(huskies, "\n");

# Using non-unique key
huskies[5] = "Bowman";
print("Using non-unique key with huskies[5] = 'Bowman'");
print(huskies, "\n");

# Adding new values to dictionary
huskies[4] = 'Sirmon';
print("Adding new value to dictionary with: huskies[4] = 'Sirmon'", "\n");

print("Dictionary length:", len(huskies), "\n");

print(huskies, "\n");

# Delete value
del huskies[5];
print("Delete values using 'del huskies[key]'", "\n");
print("Dictionary length:", len(huskies));
